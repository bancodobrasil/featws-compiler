rule feat_south_brazil salience 1002 {
    when 
        true
    then
        ctx.Put("south_brazil", processor.Boolean(result.GetBool("south_brazil_0") && result.GetBool("south_brazil_1")));
        
        result.Put("south_brazil", ctx.GetBool("south_brazil"));
        
        Retract("feat_south_brazil");
}
