rule feat_myfeat salience 1000 {
    when 
        processor.Boolean(ctx.GetInt("myothernumber") > 10); 
    then
        ctx.Put("myfeat", ctx.GetInt("mynumber") + 12 + "");
        
        result.Put("myfeat", ctx.Get("myfeat"));
        
        Retract("feat_myfeat");
}
