rule DefaultValues salience 10000000 {
    when 
        true
    then
        
        result.Put("male", "true");

        result.Put("female", "true");
        
        Retract("DefaultValues");
}

rule feat_female salience 1001 {
    when 
        true
    then
        ctx.Put("female", processor.Boolean(ctx.GetString("gender") == "F"));
        
        result.Put("female", ctx.Get("female"));
        
        Retract("feat_female");
}

rule feat_male salience 1000 {
    when 
        true
    then
        ctx.Put("male", processor.Boolean(!result.GetBool("female")));

        result.Put("male", ctx.Get("male"));

        Retract("feat_male");
}
