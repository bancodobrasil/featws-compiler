rule DefaultValues salience 10000000 {
    when 
        true
    then
    
        Retract("DefaultValues");
}

rule feat_idade_squared salience 1000 {
    when 
        true 
    then
        ctx.Put("idade_squared", ctx.GetInt("idade") * ctx.GetInt("idade"));
        
        result.Put("idade_squared", ctx.Get("idade_squared"));
        
        Retract("feat_idade_squared");
}
